    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "quality": "yarn type-check && yarn lint:check && yarn prettier",
    "pre-commit": "lint-staged",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setDevelopment": "ENVFILE=.env.dev",
    "setStaging": "ENVFILE=.env.stg",
    "setProduction": "ENVFILE=.env.prod",
    "android:dev": "yarn setDevelopment && react-native run-android --variant=devDebug",
    "android:stg": "yarn setStaging && react-native run-android --variant=stgDebug",
    "android:prod": "yarn setProduction && react-native run-android --variant=prodRelease",
    "android:clean": "cd android && ./gradlew clean",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios --scheme dev",
    "ios:stg": "ENVFILE=.env.stg react-native run-ios --scheme stg",
    "ios:prod": "ENVFILE=.env.prod react-native run-ios --scheme prod",
    "postinstall": "patch-package"
