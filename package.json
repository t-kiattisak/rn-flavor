{
  "name": "rn-flavor",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "setDevelopment": "ENVFILE=.env.development",
    "setProduction": "ENVFILE=.env.production",
    "aos:dev": "yarn setDevelopment && react-native run-android --mode=developmentdebug --appIdSuffix=dev",
    "aos:dev-release": "yarn setDevelopment && react-native run-android --mode=developmentrelease --appIdSuffix=dev",
    "aos:prod": "yarn setProduction && react-native run-android --mode=productiondebug --appIdSuffix=prod",
    "aos:prod-release": "yarn setProduction && react-native run-android --mode=productionrelease --appIdSuffix=prod",
    "aos:clean": "cd android && ./gradlew clean",
    "aos:DR-apk": "yarn setDevelopment && yarn aos:clean && cd android && ./gradlew assembleDevelopmentRelease",
    "aos:PR-apk": "yarn setProduction && yarn aos:clean && cd android && ./gradlew assembleProductionRelease",
    "aos:DD-apk": "yarn setDevelopment && yarn aos:clean && cd android && ./gradlew assembleDevelopmentDebug",
    "aos:PD-apk": "yarn setProduction && yarn aos:clean && cd android && ./gradlew assembleProductionDebug"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.80.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-config": "1.5.5"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/runtime": "7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/gradle-plugin": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@types/jest": "29.5.13",
    "@types/react": "19.1.0",
    "@types/react-test-renderer": "19.1.0",
    "eslint": "8.56.0",
    "eslint-plugin-ft-flow": "^3.0.11",
    "jest": "29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "@types/react": "19.1.0",
    "@types/react-native": "0.80.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "eslint": "8.56.0",
    "metro": "0.80.0",
    "metro-config": "0.80.0",
    "metro-core": "0.80.0",
    "metro-runtime": "0.80.0",
    "metro-source-map": "0.80.0",
    "metro-transform-worker": "0.80.0",
    "metro-transform-plugins": "0.80.0"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
